{
  "name": "spellbook",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "eslint-plugin-prettier": "^2.6.0",
    "history": "^4.7.2",
    "husky": "^1.0.0-rc.8",
    "lint-staged": "^7.1.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.16.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.3",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "reselect": "^3.0.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.1.0",
    "thenby": "^1.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.{scss}": [
      "stylelint --config=.stylelintrc --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
